                                            if (codeValue.trim() !== "") {
                                              fetch(`/applyreferel?referel=${codeValue}`, {
                                                method: "POST",
                                                headers: {
                                                  "Content-Type": "application/json",
                                                },
                                              })
                                                .then((response) => {
                                                  if (!response.ok) {
                                                    throw new Error(`HTTP error!`);
                                                  }
                                                  return response.json();
                                                })
                  
                                                .then((data) => {
                                                  console.log(data);
                                                  Swal.fire({
