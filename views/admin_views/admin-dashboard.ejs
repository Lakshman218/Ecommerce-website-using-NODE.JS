            <div class="card mb-4">
                <header class="card-header row align-items-center">
                    <div class="col-md-6">
                        <h4 class="card-title">Latest orders</h4>
                    </div>
                    <div class="col-md-6 d-flex justify-content-end">
                        <form action="/admin/filter-order" method="post" class="row align-items-center">
                            <div class="col-md-8">
                                <div class="custom_select">
                                    <select class="form-select select-nice" name="filteroption">
                                        <option disabled selected>Status</option>
                                        <option>All</option>
                                        <option>Order Placed</option>
                                        <option>Delivered</option>
                                        <option>Returned</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <button type="submit" class="btn btn-sm">Filter</button>
                            </div>
                        </form>
                    </div>
                </header>
                <div class="card-body">
                    <div class="table-responsive">
                        <div class="table-responsive">
                            <table class="table align-middle table-nowrap mb-0" id="orderTable">
                                <thead class="table-light">
                                    <tr>
                                        
                                        <th class="align-middle" scope="col">Order ID</th>
                                        <th class="align-middle" scope="col">Date</th>
                                        <th class="align-middle" scope="col">Payment Status</th>
                                        <th class="align-middle" scope="col">Payment Method</th>
                                        <th class="align-middle" scope="col">Payment Status</th>
                                        <th class="align-middle" scope="col">Total</th>
                                        <th class="align-middle" scope="col">View Details</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if(filteredData && filteredData.length > 0) {%>
                                        <% filteredData.forEach(order => {%>
                                            
                                    <tr>

                                        <td><a href="#" class="fw-bold"><%= order._id %></a> </td>
                                        
                                        <td>
                                            <%= order.createdAt.toLocaleDateString() %>
                                        </td>
                                        <td>
                                            <i class=  font-xxl text-muted mr-5></i> <%= order.orderStatus %>
                                        </td>
                                        <td>
                                            <i class=  font-xxl text-muted mr-5></i> <%= order.paymentMethod %>
                                        </td>
                                        <td>
                                            <span class="<%= order.paymentStatus === 'Success' ? 'badge-soft-success' : (order.paymentStatus === 'Pending' ? 'badge-soft-info' : 'badge-soft-danger') %>"><%= order.paymentStatus %></span>
                                        </td>
                                        <td>
                                            <%= order.totalAmount.toFixed(0) %>
                                        </td>
                                        <td>
                                            <a href="/admin/order-manage/<%= order._id %>" class="btn btn-xs"> View details</a>
                                        </td>
                                    </tr>
                                    
                                        <%}) %>    
                                    <%} else {%>
                                        <td colspan="6"><h6 class="text-center p-3" >No orders available</h6></td>
                                        <%} %>
                                    
                                </tbody>
                            </table>
                        </div>
                    </div> 
                </div>
            </div>

            <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
            <script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js"></script>
            <script src="https://cdn.datatables.net/1.13.6/js/dataTables.bootstrap5.min.js"></script>
            <script>
                $(document).ready(function () {
                    new DataTable('#orderTable', {
                        order: [[0, 'desc']] 
                    });
                });
